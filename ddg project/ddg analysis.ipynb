    "#adding values to the database\n",
    "def create_query(df):\n",
    "    query = []\n",
    "    for i,j in df.iterrows():\n",
    "        query.append(tuple(j))\n",
    "    return tuple(query)\n",
    "val = create_query(data_gdp)\n",
    "con = get_connection()\n",
    "con.execute('Delete from nsdv')\n",
    "for val in create_query(data_gdp):\n",
    "    con.execute(f'insert into nsdv values {val}')\n"
