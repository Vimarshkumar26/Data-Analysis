    "        yea.append(gdp_year[j['Year'].split('-')[1]])\n",
    "data_gdp = data.copy()\n",
    "data_gdp.insert(14,'GDP_OF_INDIA',value = yea,allow_duplicates = True)\n",
    "gdp_percent = {}\n",
    "for i in data_gdp.iterrows():\n",
    "    gdp_percent[i[1]['States']] = (i[1]['TOTAL NSVA at basic prices']/i[1]['GDP_OF_INDIA'])*100\n",
