    "#descriptive analysis\n",
    "#find states for min max contribution  for each attribute year wise\n",
    "#index:year  = 0:2017-18 , 1:2018-2019 , 2:2019-2020 , 3:2020-2021,4: 2021-2022\n",
    "from tabulate import tabulate\n",
    "def find_min_max(index):\n",
    "    max_val_dict = {}\n",
    "    min_val_dict={}\n",
    "    max_val = groups_year[index].iloc[:,2:].idxmax()\n",
    "    min_val = groups_year[index].iloc[:,2:].idxmin()\n",
    "    for i,j in max_val.items():\n",
    "        max_val_dict[i] = [groups_year[index].iloc[j,0],groups_year[index].loc[j,i]]\n",
    "    for i,j in min_val.items():\n",
    "        min_val_dict[i] = [groups_year[index].iloc[j,0],groups_year[index].loc[j,i]]\n",
    "    return {\"max\" : pd.DataFrame(max_val_dict,index = ['state_highest_value','max_value']).T,\"min\":pd.DataFrame(min_val_dict,index = ['state_minimum_value','min_value']).T}\n",
    "max_data =find_min_max(4)['max']\n",
    "\n",
    "for i in range(0,4):\n",
    "    print(tabulate(find_min_max(i)['min'], headers = 'keys', tablefmt = 'pretty'))\n",
    "    print(tabulate(find_min_max(i)['max'], headers = 'keys', tablefmt = 'pretty'))   "
   ]
