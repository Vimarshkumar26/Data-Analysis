    "#functions to create single and multi line graphs\n",
    "def get_line(col):\n",
    "    values = {}\n",
    "    for i in year_states.keys():\n",
    "        values[i] ={'val':[],'year':[]} \n",
    "        val1 = []\n",
    "        yearlist =list(year_states[i].keys())\n",
    "        for j in yearlist:\n",
    "            values[i]['val'].append(year_states[i][j][col])\n",
    "            values[i]['year'].append(j)\n",
    "    return values\n",
    "def get_multi(col):\n",
    "    list4 = get_line(col)\n",
    "    yval = []\n",
    "    xyear = []\n",
    "    for i in list4.keys():\n",
    "        xyear.append(list4[i]['year'])\n",
    "        yval.append(list4[i]['val'])\n",
    "\n",
    "    data = []\n",
    "    for i in range(len(list4.keys())):\n",
    "        data.append(go.Scatter(x=xyear[i],y=yval[i],name = list(list4.keys())[i]))\n",
    "\n",
    "    layout = go.Layout(title=col)\n",
    "\n",
    "    fig = go.Figure(data=data, layout=layout)\n",
    "    return fig\n"
   ]
