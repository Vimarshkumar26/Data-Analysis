    "\n",
    "def get_connection():\n",
    "    return db.create_engine(\n",
    "        url=\"mysql+pymysql://{0}:{1}@{2}:{3}/{4}\".format(\n",
    "            user, password, host, port, database\n",
    "        ),echo=False\n",
    "    )\n",
    "try:\n",
    "\n",
    "    # GET THE CONNECTION OBJECT (ENGINE) FOR THE DATABASE\n",
    "    engine = get_connection()\n",
    "    print(\n",
    "        f\"Connection to the {host} for user {user} created successfully.\")\n",
    "except Exception as ex:\n",
    "    print(\"Connection could not be made due to the following error: \\n\", ex)\n",
    "    \n",
    "\n",
    "engine.execute('drop table if exists nsdv ')\n",
    "#con.execute('drop table if exists nsdv')\n",
    "    \n",
    "meta = db.MetaData()\n",
    "    \n",
    "  \n",
    "# Define the profile table\n",
    "# database name\n",
    "nsdv = db.Table(\n",
    "    'nsdv',                                        \n",
    "    meta,                                    \n",
    "    db.Column('States', db.String(20)),  \n",
    "    db.Column('Year', db.String(20)),                    \n",
    "    db.Column('Agriculture', db.Integer),        \n",
    "    db.Column('Mining', db.Integer),\n",
    "    db.Column('Manufacturing', db.Integer),\n",
    "    db.Column('Utility_service', db.Integer),\n",
    "    db.Column('Construction', db.Integer),\n",
    "    db.Column('Trade_Hotel', db.Integer),\n",
    "    db.Column('Transport_Communication', db.Integer),\n",
    "    db.Column('Financial_services', db.Integer),\n",
    "    db.Column('Real_estate', db.Integer),\n",
    "    db.Column('Public_admin', db.Integer),\n",
    "    db.Column('Others', db.Integer),\n",
    "    db.Column('NSDV', db.Integer),\n",
    "    db.Column('GDP', db.Integer)\n",
    ")\n",
    "meta.create_all(engine)"
